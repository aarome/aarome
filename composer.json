{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "url": "https://github.com/previousnext/drush_cmi_tools.git",
            "type": "git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/address": "1.x-dev",
        "drupal/admin_toolbar": "^1.24",
        "drupal/admin_toolbar_tools": "^1.24",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/adminimal_theme": "^1.3",
        "drupal/allowed_formats": "^1.1",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/backup_migrate": "^4.0",
        "drupal/better_exposed_filters": "3.0-alpha4",
        "drupal/blazy": "^2.0",
        "drupal/block_class": "^1.0",
        "drupal/cer": "^4.0@alpha",
        "drupal/chosen": "^2.6",
        "drupal/colorbox": "^1.4",
        "drupal/computed_field": "2.0-beta1",
        "drupal/config_devel": "^1.2",
        "drupal/config_tools": "^1.0@alpha",
        "drupal/context": "4.x-dev",
        "drupal/context_active_trail": "^1.0@RC",
        "drupal/core": "^8.8.1",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/date_popup": "^1.0",
        "drupal/daterange_compact": "1.x-dev",
        "drupal/datetime_extras": "1.x-dev",
        "drupal/devel_entity_updates": "^1.0",
        "drupal/dropdown_language": "^2.2",
        "drupal/ds": "^3.3",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/empty_page": "^2.0",
        "drupal/entity": "^1.0",
        "drupal/entity_form_field_label": "^1.3",
        "drupal/entity_language_fallback": "^1.2",
        "drupal/facets": "^1.4",
        "drupal/facets_block": "^1.2",
        "drupal/filefield_paths": "1.x-dev",
        "drupal/hacked": "^2.0@beta",
        "drupal/hide_revision_field": "^2.1",
        "drupal/honeypot": "^1.29",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imageapi_optimize_binaries": "^1.0@alpha",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "5.0-beta8",
        "drupal/menu_block": "^1.5",
        "drupal/menu_export": "^1.2",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_trail_by_path": "1.x-dev",
        "drupal/metatag": "^1.7",
        "drupal/name": "^1.0@RC",
        "drupal/node_edit_protection": "^1.0@alpha",
        "drupal/noreqnewpass": "^1.1",
        "drupal/obfuscate": "^1.0@alpha",
        "drupal/optional_end_date": "^1.0@beta",
        "drupal/override_node_options": "^2.4",
        "drupal/owlcarousel": "^1.0@alpha",
        "drupal/paragraphs": "1.x-dev",
        "drupal/pathauto": "^1.6",
        "drupal/responsive_favicons": "^1.4",
        "drupal/responsive_menu": "^2.6",
        "drupal/search_api": "^1.16",
        "drupal/search_api_solr": "^3.9",
        "drupal/selective_better_exposed_filters": "^1.0@beta",
        "drupal/slick": "2.0-rc4",
        "drupal/slick_media": "2.0-alpha2",
        "drupal/slick_views": "^2.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/spamspan": "^1.1",
        "drupal/tmgmt": "^1.6",
        "drupal/toolbar_anti_flicker": "^3.1",
        "drupal/twig_tweak": "^2.4",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/views_bulk_operations": "^3.2",
        "drupal/views_conditional": "^1.0",
        "drupal/views_contextual_filters_or": "^1.1",
        "drupal/views_fieldset": "1.x-dev",
        "drupal/views_fieldsets": "^3.3",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/views_rows_wrapper": "^1.1",
        "drupal/views_slideshow": "^4.6",
        "drupal/views_url_alias": "^1.0",
        "drupal/views_url_path_arguments": "1.x-dev",
        "drupal/webform": "^5.0@RC",
        "drupal/webp": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "npm-asset/blazy": "^1.8",
        "npm-asset/slick-carousel": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1",
        "twig/twig": "1.42.5",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.1",
        "drupal/devel": "^1.2",
        "geerlingguy/drupal-vm": "^4.9",
        "symfony/debug": "^3.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "library",
            "npm-asset"
        ],
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "allowed-packages": [
                "drupal/core"
            ],
            "file-mapping": {
                "[web-root]/.htaccess": {
                    "mode": "replace",
                    "path": "web/core/assets/scaffold/files/htaccess",
                    "overwrite": false
                },
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "web/core/assets/scaffold/files/development.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "web/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "web/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "web/libraries/blazy": [
                "npm-asset/blazy"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
