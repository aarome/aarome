{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/admin_toolbar": "^1.24",
        "drupal/admin_toolbar_tools": "^1.24",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/adminimal_theme": "^1.3",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/better_exposed_filters": "3.0-alpha4",
        "drupal/cer": "^4.0@alpha",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_devel": "^1.2",
        "drupal/console": "^1.0.2",
        "drupal/context": "^4.0@beta",
        "drupal/core": "~8.5.3",
        "drupal/delete_all": "^1.0",
        "drupal/entity_language_fallback": "^1.2",
        "drupal/filefield_paths": "1.x-dev",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "^4.3",
        "drupal/menu_block": "^1.5",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/migrate_plus": "4.0-beta3",
        "drupal/migrate_run": "^1.0@beta",
        "drupal/migrate_source_csv": "^2.2",
        "drupal/name": "^1.0@RC",
        "drupal/noreqnewpass": "^1.1",
        "drupal/paragraphs": "1.x-dev",
        "drupal/pathauto": "^1.2",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api": "^1.9",
        "drupal/search_api_solr": "^2.0",
        "drupal/slick": "^1.0",
        "drupal/slick_media": "2.0-alpha2",
        "drupal/slick_views": "^1.0@RC",
        "drupal/structure_sync": "^1.12",
        "drupal/tmgmt": "^1.6",
        "drupal/views_url_alias": "^1.0",
        "drupal/views_url_path_arguments": "1.x-dev",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "geerlingguy/drupal-vm": "^4.9",
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
